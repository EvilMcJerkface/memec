#!/usr/bin/gnuplot

set terminal pngcairo size 800,600 enhanced font 'Arial, 12'

set output 'fairness_maxmin.png'

set title "Max-Min Average Load vs. number of stripe lists (c)" font 'Arial Bold'
set xlabel "Number of stripe lists (c)" font 'Arial Bold'
set ylabel "Max-Min Average Load" font 'Arial Bold'

plot 'results.txt' using 1:2 title 'Stripe List Generation Algorithm' with linespoints

################################################################################

set output 'fairness_stdev.png'

set title "Standard Deviation of Load vs. number of stripe lists (c)" font 'Arial Bold'
set xlabel "Number of stripe lists (c)" font 'Arial Bold'
set ylabel "Standard Deviation of Load" font 'Arial Bold'

plot 'results.txt' using 1:3 title 'Stripe List Generation Algorithm' with linespoints

################################################################################

set output 'fairness_jains.png'

set title "Jain's Fairness of Load vs. number of stripe lists (c)" font 'Arial Bold'
set xlabel "Number of stripe lists (c)" font 'Arial Bold'
set ylabel "Jain's Fairness of Load" font 'Arial Bold'

plot 'results.txt' using 1:4 title 'Stripe List Generation Algorithm' with linespoints
